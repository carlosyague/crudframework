<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib>
	<namespace>http://myfaces.apache.org/sandbox</namespace>
	<tag>
		<tag-name>selectOneRow</tag-name>
		<component>
			<component-type>
				org.apache.myfaces.SelectOneRow
			</component-type>
			<renderer-type>
				org.apache.myfaces.SelectOneRow
			</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>modalDialog</tag-name>
		<component>
			<component-type>
				org.apache.myfaces.ModalDialog
			</component-type>
			<renderer-type>
				org.apache.myfaces.ModalDialog
			</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>inputSuggestAjax</tag-name>
			<component>
				<!-- component-type>org.apache.myfaces.custom.suggestajax.inputsuggestajax.InputSuggestAjax</component-type -->
				<!-- renderer-type>org.apache.myfaces.custom.suggestajax.inputsuggestajax.InputSuggestAjax</renderer-type-->
				<component-type>
					org.apache.myfaces.InputSuggestAjax
				</component-type>
				<renderer-type>
					org.apache.myfaces.InputSuggestAjax
				</renderer-type>
				<handler-class>
					facelets.InputSuggestAjaxComponentHandler
				</handler-class>
			</component>
	</tag>

</facelet-taglib>
